{
  "name": "QuestDB",
  "short_name": "questdb",
  "is_self": false,
  "description": "Open-source time-series database targeting capital markets and IoT",

  "sources": {
    "docs": [
      {
        "id": "questdb-docs-main",
        "base_url": "https://questdb.io/docs/",
        "scrape_method": "crawl",
        "content_selector": "article",
        "max_depth": 2,
        "max_pages": 75,
        "rate_limit_seconds": 0.3,
        "exclude_patterns": [
          "/docs/third-party-tools/",
          "/docs/clients/"
        ]
      },
      {
        "id": "questdb-docs-sql",
        "base_url": "https://questdb.io/docs/reference/sql/",
        "scrape_method": "crawl",
        "content_selector": "article",
        "max_depth": 2,
        "max_pages": 40,
        "rate_limit_seconds": 0.3
      },
      {
        "id": "questdb-capital-markets",
        "base_url": "https://questdb.com/enterprise/capital-markets/",
        "scrape_method": "single_page",
        "content_selector": "main"
      },
      {
        "id": "questdb-pricing",
        "base_url": "https://questdb.com/pricing/",
        "scrape_method": "single_page",
        "content_selector": "main"
      }
    ],

    "blog": {
      "base_url": "https://questdb.io/blog/",
      "scrape_method": "crawl",
      "content_selector": "article",
      "max_pages": 30,
      "rate_limit_seconds": 0.3,
      "priority_keywords": ["benchmark", "performance", "capital markets", "trading", "financial", "release", "enterprise"]
    },

    "github": {
      "repos": [
        {
          "repo": "questdb/questdb",
          "scrape_issues": true,
          "scrape_discussions": true,
          "scrape_releases": true,
          "max_issues": 200,
          "max_discussions": 100,
          "issue_sort": "comments",
          "issue_direction": "desc",
          "fetch_comments_for_top_n": 20,
          "labels_of_interest": ["bug", "enhancement", "feature request", "breaking change"]
        }
      ]
    },

    "community": {
      "reddit": {
        "search_terms": [
          "QuestDB",
          "questdb vs",
          "questdb problem",
          "questdb limitation",
          "questdb performance",
          "questdb trading",
          "questdb financial"
        ],
        "subreddits": ["databases", "devops", "algotrading", "quant"],
        "max_results_per_query": 50
      },
      "hackernews": {
        "search_terms": [
          "QuestDB",
          "questdb"
        ],
        "max_results_per_query": 50
      }
    },

    "benchmarks": [
      {
        "name": "clickbench",
        "url": "https://benchmark.clickhouse.com/",
        "scrape_method": "single_page"
      },
      {
        "name": "tsbs_blog",
        "url": "https://questdb.io/blog/tags/benchmark/",
        "scrape_method": "crawl",
        "max_depth": 2
      },
      {
        "name": "kx_tsbs_benchmark",
        "url": "https://kx.com/blog/benchmarking-kdb-x-vs-questdb-clickhouse-timescaledb-and-influxdb-with-tsbs/",
        "scrape_method": "single_page"
      },
      {
        "name": "medium_comparison",
        "url": "https://medium.com/@smakhnist/picking-the-fastest-database-to-store-time-series-data-411ca3651277",
        "scrape_method": "single_page"
      }
    ]
  },

